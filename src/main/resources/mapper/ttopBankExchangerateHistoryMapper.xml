<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jrong.dataCollector.dao.ttopBankExchangerateHistoryMapper">

    <select id="GetttopBankExchangerateHistory">
        SELECT * FROM ttop_bank_exchangerate_history;
    </select>

    <insert id="SavettopBankExchangerateHistory" parameterType="java.util.List">
        INSERT INTO ttop_bank_exchangerate_history(bankCode, exchangeContent, createDate) VALUES
        <foreach item="item" index="index" collection="ttopBankExchangerateHistoryList" separator=",">
            (#{item.bankCode}, #{item.exchangeContent}, #{item.createDate})
        </foreach>
    </insert>

    <delete id="DeletettopBankExchangerateHistory">
        DELETE FROM ttop_bank_exchangerate_history WHERE id = #{id};
    </delete>

    <update id="UpdatettopBankExchangerateHistory">
        UPDATE ttop_bank_exchangerate_history SET exchangeContent = #{exchangeContent} WHERE id = #{id};
    </update>
</mapper>